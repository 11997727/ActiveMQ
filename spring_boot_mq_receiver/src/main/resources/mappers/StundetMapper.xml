<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot_mq_receiver.mapper.StudentMapper">

    <select id="selectSnameBySid" resultType="java.lang.String">
        select sname from student where sid=#{sid}
    </select>
</mapper>